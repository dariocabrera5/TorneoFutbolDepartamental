@page
@model TorneoFutbolDepartamental.App.Frontend.pages.Municipios.IndexModel
@{
}
<h1>Listado de Municipios</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Nombre</th>
            <th scope="col">Acciones</th>
        </tr>   
    </thead>
    <tbody>
        @foreach (var municipio in Model.municipios)
        {
            <tr>
                <td>@municipio.Nombre</td>
                <td><a class="btn btn-primary" asp-page="./Details" asp-route-Municipioid="@municipio.MunicipioId">Detalles</a></td>
            </tr>
        }
    </tbody>
</table>
