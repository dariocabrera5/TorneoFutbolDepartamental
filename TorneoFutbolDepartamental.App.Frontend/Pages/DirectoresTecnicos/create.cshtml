@page
@model TorneoFutbol.App.Frontend.pages.DirectoresTecnicos.createModel
@{
}
<h1>Nuevo Director Tecnico</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="directortecnico.Nombre"></label>
        <input asp-for="directortecnico.Nombre" class="form-control" placeholder="pepito perez">
    </div>
    <div class="form-group">
        <label asp-for="directortecnico.Documento"></label>
        <input asp-for="directortecnico.Documento" class="form-control">
    </div>
     <div class="form-group">
        <label asp-for="directortecnico.Telefono"></label>
        <input asp-for="directortecnico.Telefono" class="form-control">
    </div>
     <div class="form-group">
        <label asp-for="directortecnico.Equipo"></label>
        <input asp-for="directortecnico.Equipo" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary btn-block">Añadir Arbitro</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>