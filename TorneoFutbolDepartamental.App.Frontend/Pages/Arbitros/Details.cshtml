@page
@model TorneoFutbolDepartamental.App.Frontend.pages.Arbitros.DetailsModel
@{
}
<h1>Detalles Del Arbitro</h1>
<table class="table table-dark">
        <tr>
            <th scope="col">Nombre</th>
            <td>@Model.arbitro.Nombre</td>
        </tr>
        <tr>
            <th scope="col">Documento</th>
            <td>@Model.arbitro.Documento</td>
        </tr>
        <tr>
            <th scope="col">Telefono</th>
            <td>@Model.arbitro.Telefono</td>
        </tr>
        <tr>
            <th scope="col">ColegioPertenece</th>
            <td>@Model.arbitro.ColegioPertenece</td>
        </tr>
</table>
<a class="btn btn-primary" asp-page="./Index">Volver</a>